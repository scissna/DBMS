--------------INSERT VALUES INTO PRODUCT TABLE-------------------


mysql> INSERT INTO PRODUCT VALUES(101,'PEN',100);
Query OK, 1 row affected (0.06 sec)

mysql> INSERT INTO PRODUCT VALUES(102,'BOOK',120);
Query OK, 1 row affected (0.05 sec)

mysql> INSERT INTO PRODUCT VALUES(103,'BOX',30);
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO PRODUCT VALUES(104,'PENCIL',60);
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO PRODUCT VALUES(105,'SCALE',50);
Query OK, 1 row affected (0.09 sec)

mysql> select *from PRODUCT;
+-----+--------+--------+
| pid | pname  | price  |
+-----+--------+--------+
| 101 | PEN    | 100.00 |
| 102 | BOOK   |  60.00 |
| 103 | BOX    |  30.00 |
| 104 | PENCIL |  60.00 |
| 105 | SCALE  |  50.00 |
+-----+--------+--------+
5 rows in set (0.00 sec)




---------------INSERT VALUES INTO INVOIVE_MASTER TABLE----------------------


mysql> INSERT INTO INVOICE_MASTE values(1001,1,'2022-01-01');
Query OK, 1 row affected (0.05 sec)

mysql> INSERT INTO INVOICE_MASTE values(1002,2,'2022-01-31');
Query OK, 1 row affected (0.07 sec)

mysql> INSERT INTO INVOICE_MASTE values(1003,3,'2022-03-31');
Query OK, 1 row affected (0.04 sec)

mysql> INSERT INTO INVOICE_MASTE values(1004,4,'2022-02-28');
Query OK, 1 row affected (0.08 sec)

mysql> INSERT INTO INVOICE_MASTE values(1005,5,'2022-05-2');
Query OK, 1 row affected (0.08 sec)

mysql> select * from INVOICE_MASTE;
+--------+---------+------------+
| inv_id | cust_id | inv_date   |
+--------+---------+------------+
|   1001 |       1 | 2022-01-01 |
|   1002 |       2 | 2022-01-31 |
|   1003 |       3 | 2022-03-31 |
|   1004 |       4 | 2022-02-28 |
|   1005 |       5 | 2022-05-02 |
+--------+---------+------------+
5 rows in set (0.00 sec)






--------------------INSERT VALUS INTO INOICE_ITEM TABLE--------------------------

mysql> INSERT INTO INOVICE_ITEM VALUES(1,101,5);
Query OK, 1 row affected (0.05 sec)

mysql> INSERT INTO INOVICE_ITEM VALUES(2,101,2);
Query OK, 1 row affected (0.05 sec)

mysql> INSERT INTO INOVICE_ITEM VALUES(2,103,4);
Query OK, 1 row affected (0.05 sec)

mysql> INSERT INTO INOVICE_ITEM VALUES(3,102,10);
Query OK, 1 row affected (0.06 sec)

mysql> INSERT INTO INOVICE_ITEM VALUES(5,105,9);
Query OK, 1 row affected (0.06 sec)

mysql> INSERT INTO INOVICE_ITEM VALUES(5,104,8);
Query OK, 1 row affected (0.46 sec)



mysql> SELECT * FROM INOVICE_ITEM;
+--------+------+----------+
| inv_id | pid  | quantity |
