mysql> INSERT INTO CUST VALUES(2,'SCISSMO','THEKKEPURATH','KKM',907456750);
Query OK, 1 row affected (0.46 sec)



mysql> INSERT INTO CUST VALUES(3,'AMRUTHA','MADHUCHIRAYIL','TSR',907456752);
Query OK, 1 row affected (0.08 sec)



mysql> INSERT INTO CUST VALUES(4,'SARANYA','KANNOTH','CALICUT',907451750);
Query OK, 1 row affected (0.04 sec)



mysql> INSERT INTO CUST VALUES(5,'HASNA','ETTAMTHARAYIL','PKD',607451750);
Query OK, 1 row affected (0.46 sec)


mysql> INSERT INTO CUST(id,name,address,city,ph_no)  VALUES(6,'VARUN','KALLAYIL','EKM',' ');
Query OK, 1 row affected (0.08 sec)



mysql> SELECT *FROM CUST;
